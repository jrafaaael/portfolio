---
import type { HTMLAttributes } from "astro/types";

interface Props extends Omit<HTMLAttributes<"a">, "class"> {
  className?: string;
}

const { className = "", ...props } = Astro.props;
---

<a
  class={`text-rose-100/70 transition hover:text-rose-100/90 ${className}`}
  {...props}><slot /></a
>
